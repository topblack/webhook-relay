{
  "name": "webhook-relay",
  "version": "1.0.0",
  "description": "Relay webhook event from internet to local network.",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "external": "node dist/external",
    "internal": "node dist/internal",
    "docker-build": "docker build -t webhook-relay .",
    "docker-run-int": "docker run --rm --name webhook-relay-int -p 8000:3000 webhook-relay internal",
    "docker-run-ext": "docker run --rm --name webhook-relay-ext -p 8001:3000 webhook-relay external"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/topblack/webhook-relay.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/topblack/webhook-relay/issues"
  },
  "homepage": "https://github.com/topblack/webhook-relay#readme",
  "dependencies": {
    "body-parser": "^1.17.2",
    "express": "^4.15.3",
    "node-rsa": "^1.0.7",
    "restler": "^3.4.0",
    "socket.io": "^2.0.3"
  },
  "devDependencies": {
    "@types/node": "^12.12.21",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4"
  }
}
